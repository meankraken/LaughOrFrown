@model IEnumerable<LaughOrFrown.ViewModels.JokeViewModel>

@{
    Layout = "_Layout";
}


<div id="jokesView">
    <div class="jokeBox">
        <h3>Why did the chicken cross the road?</h3>
    </div>
    <div class="jokeBox">
        <h3>Why did the chicken cross the road testing testing testing blah blah blah blah blahWhy did the chicken cross the road testing testing testing blah blah blah blah blahWhy did the chicken cross the road testing testing testing blah blah blah blah blah</h3>
    </div>
    <div class="jokeBox">
        <h3>Is this the real life, or is this just fantasy?</h3>
    </div>

    @foreach (var Joke in Model)
    {
        <a asp-action="Joke" asp-route-id=@Joke.Id asp-route-returnurl="@Context.Request.Path">
            <div class="jokeBox">
                <h3>@Joke.JokeText</h3>
                <h6>@Joke.OffensiveAverageRating</h6>
            </div>
        </a>

    }

</div>






